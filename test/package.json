{
  "name": "grpc-web-test",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:testserver": "cd go && go build -o ./build/testserver ./testserver/testserver.go",
    "build:ts": "cd ts && webpack",
    "build:dev": "cd ts && webpack --watch",
    "build:proto": "./protogen_go.sh && ./protogen_ts.sh",
    "build": "npm run build:proto && npm run build:testserver && npm run build:ts",
    "lint": "tslint -c ./ts/tslint.json ./ts/src/**/*.ts",
    "test": "npm run build && ./run-karma.sh --single-run",
    "test:dev": "npm run build && ./run-karma.sh"
  },
  "license": "none",
  "dependencies": {
    "@types/node": "^7.0.5",
    "@types/google-protobuf": "^3.2.1",
    "google-protobuf": "^3.2.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.13.2",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-jasmine": "^1.0.2",
    "karma-sauce-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "ts-loader": "^2.0.1",
    "tslint": "^4.4.2",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1"
  }
}
